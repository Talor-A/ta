{
  "name": "",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "cf-typegen": "wrangler types",
    "db:generate": "dotenv -- drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply --local DB",
    "db:migrate-production": "dotenv -- drizzle-kit migrate",
    "deploy": "bun run build && wrangler deploy",
    "dev": "react-router dev",
    "postinstall": "bun run cf-typegen && bun run prettier --write worker-configuration.d.ts",
    "format": "prettier --write .",
    "preview": "bun run build && vite preview",
    "typecheck": "bun run cf-typegen && react-router typegen && tsc -b",
    "wrangler": "wrangler"
  },
  "dependencies": {
    "@atcute/bluesky": "^3.2.0",
    "better-auth": "^1.3.4",
    "drizzle-orm": "~0.36.3",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-router": "^7.7.1",
    "remark-gfm": "^4.0.1",
    "turndown": "^7.2.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.0.12",
    "@react-router/dev": "^7.7.1",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "dotenv-cli": "^7.4.3",
    "drizzle-kit": "~0.28.1",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.13.2"
  }
}
